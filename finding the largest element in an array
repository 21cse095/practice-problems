import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int N = sc.nextInt();
        int[] arr = new int[N];
        for(int i=0;i<N;i++){
            arr[i]=sc.nextInt();
        }
        int max = arr[0];
        for(int num:arr){
            if(num>max)
              max=num;
        }
        System.out.println("The largest element in the array is :"+max);
    }
}
